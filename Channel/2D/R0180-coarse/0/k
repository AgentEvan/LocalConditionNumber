FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];


internalField   uniform 0.375;

boundaryField
{
    INLET
    {
        type            cyclic;
    }
    OUTLET
    {
        type            cyclic;
    }
    WALL
    {
        type            fixedValue;
        value           uniform 1e-10;
    }
    CENTER
    {
        type            symmetryPlane;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}


